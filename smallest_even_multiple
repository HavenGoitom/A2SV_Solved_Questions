class Solution(object):
    def smallestEvenMultiple(self, n):
        """
        :type n: int
        :rtype: int
        """
        if(n%2 == 0):
            for i in range(2,2*n):
                if(i%n == 0):
                    if(i%2 == 0):
                        return i
        else:
            return 2*n
        
