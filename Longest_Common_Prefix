class Solution(object):
    def longestCommonPrefix(self, strs):
        if not strs:
            return ""

        min_len = len(strs[0])

        for s in strs:
            if len(s) < min_len:
                min_len = len(s)
        res = ""
s
        for i in range(min_len):
            for j in range(1, len(strs)):
                if strs[0][i] != strs[j][i]:
                    return res
            res += strs[0][i]

        return res
